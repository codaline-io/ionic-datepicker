    "generate": "stencil generate",
    "prepublishOnly": "npm run build",
    "build:deploy": "npm run build && cp ./src/index.html ./dist && gh-pages -d ./dist"
  },
  "devDependencies": {
    "@ionic/core": "^5.0.5",
    "@stencil/core": "^1.11.0",
    "gh-pages": "^2.2.0",
    "js-datepicker": "^5.4.0"
  },
  "peerDependencies": {
    "@ionic/core": "^5.0.5",
    "js-datepicker": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codaline-io/ionic-datepicker.git"
  },
  "author": "Marcus Kirsch & Bengt Wei√üe GbR (codaline)",
  "bugs": {
    "url": "https://github.com/codaline-io/ionic-datepicker"
